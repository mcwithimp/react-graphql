{
  "extends": ["react-app", "prettier"],
  "plugins": ["prettier", "react-hooks"],
  "rules": {
    "no-console": "warn",
    "prettier/prettier": "error",
    "no-debugger": "warn",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    },
    "react": {
      "version": "detect"
    }
  }
}
